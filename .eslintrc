{
  "parser": "babel-eslint",
  "env": {
  "browser": false,
    "es6": true,
    "react-native/react-native": true,
    "jest": true
},
  "extends": [
  "prettier",
  "airbnb-base",
  "eslint:recommended",
  "plugin:react/recommended",
  "plugin:prettier/recommended"
],
  "globals": {
  "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
},
  "parserOptions": {
  "ecmaFeatures": {
    "jsx": true
  },
  "ecmaVersion": 2018,
    "sourceType": "module"
},
  "plugins": [
  "react",
  "prettier",
  "react-native"
],
  "rules": {
  "prettier/prettier": "error",
  "no-console": "off",
    "react/jsx-filename-extension": [
    1,
    {
      "extensions": [
        ".js",
        ".jsx"
      ]
    }
  ],
    "react/prefer-stateless-function": "off",
    "react/prop-types": [
    "error",
    {
      "ignore": [
        "navigation"
      ]
    }
  ],
    "no-use-before-define": 0,
    "no-alert": 0,
    "quotes": [
    0,
    "single",
    "avoid-escape"
  ],
    "comma-dangle": [
    0,
    "never"
  ],
    "class-methods-use-this": 0
}
}
